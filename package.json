{
  "name": "comercializadora-cf",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:analyze": "tsc -b && vite build --mode analyze",
    "build:production": "NODE_ENV=production tsc -b && vite build --mode production",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "test:unit": "vitest run",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "cypress open",
    "test:e2e:headless": "cypress run --headless",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "e2e": "cypress open",
    "e2e:headless": "cypress run",
    "serve": "serve -s dist -l 3000",
    "clean": "rm -rf dist node_modules/.vite",
    "postbuild": "echo 'Build completed successfully!'",
    "prepare": "husky install",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@commitlint/config-conventional": "^19.8.1",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@hookform/resolvers": "^5.2.1",
    "@mui/icons-material": "^7.3.2",
    "@mui/lab": "^7.0.0-beta.17",
    "@mui/material": "^7.3.2",
    "@mui/x-data-grid": "^8.11.1",
    "@mui/x-date-pickers": "^8.11.1",
    "@testing-library/dom": "^10.4.1",
    "@types/node": "^24.3.1",
    "@types/nodemailer": "^7.0.1",
    "@types/react-router-dom": "^5.3.3",
    "date-fns": "^4.1.0",
    "depcheck": "^1.4.7",
    "es-toolkit": "^1.39.10",
    "firebase": "^12.2.1",
    "formik": "^2.4.6",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.2",
    "jspdf-autotable": "^5.0.2",
    "nodemailer": "^7.0.6",
    "notistack": "^3.0.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-firebase-hooks": "^5.1.1",
    "react-hook-form": "^7.62.0",
    "react-router-dom": "^7.8.2",
    "recharts": "^3.2.0",
    "web-vitals": "^5.1.0",
    "yup": "^1.7.0",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@eslint/js": "^9.33.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^1.3.1",
    "@vitest/ui": "^1.3.1",
    "cypress": "^13.6.6",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-testing-library": "^6.2.0",
    "globals": "^16.3.0",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "rollup-plugin-visualizer": "^6.0.3",
    "terser": "^5.44.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2",
    "vitest": "^1.3.1",
    "wait-on": "^7.2.0"
  }
}
