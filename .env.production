# Production Environment Variables
# These will be overridden by GitHub Actions secrets in CI/CD

# Firebase Configuration (will be set by GitHub Actions)
VITE_FIREBASE_API_KEY=your_production_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_production_auth_domain_here
VITE_FIREBASE_PROJECT_ID=your_production_project_id_here
VITE_FIREBASE_STORAGE_BUCKET=your_production_storage_bucket_here
VITE_FIREBASE_MESSAGING_SENDER_ID=your_production_messaging_sender_id_here
VITE_FIREBASE_APP_ID=your_production_app_id_here
VITE_FIREBASE_MEASUREMENT_ID=your_production_measurement_id_here

# Application Configuration
VITE_APP_ENV=production
VITE_APP_NAME=Comercializadora CF
VITE_APP_VERSION=1.0.0
VITE_BUILD_DATE=

# Performance Monitoring
VITE_PERFORMANCE_SAMPLE_RATE=0.1
VITE_PERFORMANCE_ENDPOINT=

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_ENABLE_PWA_FEATURES=true

# API Configuration
VITE_API_BASE_URL=https://api.comercializadora-cf.com
VITE_API_TIMEOUT=30000

# Security
VITE_ENABLE_HTTPS_ONLY=true
VITE_ENABLE_SECURITY_HEADERS=true
